<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE entry SYSTEM "SamplesInfo.dtd">
<entry>
    <name>Simple Vote Intrinsics with libNVRTC</name>
    <description><![CDATA[Simple program which demonstrates how to use the Vote (any, all) intrinsic instruction in a CUDA kernel with runtime compilation using NVRTC APIs. Requires Compute Capability 2.0 or higher.]]></description>
    <whitepaper></whitepaper>
    <supported_sm_architectures>
        <include>all</include>
    </supported_sm_architectures>
    <keywords>CUDA,GPGPU,vote,libNVRTC</keywords>
    <scope>1:CUDA Basic Topics</scope>
    <files>
    </files>
    <cudaapi>cuMemAlloc</cudaapi>
    <cudaapi>cuMemFree</cudaapi>
    <cudaapi>cuMemcpyHtoD</cudaapi>
    <cudaapi>cuMemFree</cudaapi>
    <keyconcepts level="basic">Vote Intrinsics</keyconcepts>
    <keyconcepts level="basic">CUDA Driver API</keyconcepts>
    <keyconcepts level="advanced">Runtime Compilation</keyconcepts>
    <library os="linux">cuda</library>
    <library os="windows">cuda</library>
    <library os="macosx" framework="true">CUDA</library>
    <library>nvrtc</library>
    <include-path>./</include-path>
    <include-path>../</include-path>
    <include-path>../../common/inc</include-path>
    <include-path>$(CUDA_PATH)/include</include-path>
    <devicecompilation>whole</devicecompilation>
    <type>exe</type>
    <primary-file>simpleVoteIntrinsics.cpp</primary-file>
    <required_dependencies>
        <dependency>NVRTC</dependency>
    </required_dependencies>
    <supported-env>linux:x86_64, windows7:x86_64, macosx:x86_64, linux:ppc64le</supported-env>
    <no-nsight-eclipse/>
</entry>
